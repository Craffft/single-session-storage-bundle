{
    "name": "craffft/single-session-storage-bundle",
    "type": "contao-bundle",
    "description": "SingleSessionStorageBundle for Symfony",
    "keywords": ["single", "session", "storage"],
    "homepage":"https://craffft.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kiesel",
            "homepage": "https://github.com/iCodr8"
        }
    ],
    "support": {
        "issues": "https://github.com/Craffft/single-session-storage-bundle/issues",
        "source": "https://github.com/Craffft/single-session-storage-bundle"
    },
    "require": {
        "php": ">=7.2",
        "symfony/symfony": "~4.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "phpunit/phpunit": "^8.0"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "Craffft\\SingleSessionStorageBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "": "tests/Controller/App/",
            "Tests\\Craffft\\SingleSessionStorageBundle\\": "tests/",
            "Craffft\\SingleSessionStorageBundle\\": "src/"
        }
    }
}
